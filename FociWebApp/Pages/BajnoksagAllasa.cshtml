@page
@model FociWebApp.Pages.BajnoksagAllasaModel
@{
	ViewData["Title"] = "BajnoksagAllasa";
}
<h1>Bajnokság Állása</h1>
<table class="table">
    <thead>
		<tr>
			<th>Helyezés</th>
			<th>Csapat Neve</th>
			<th>JM</th>
			<th>Gy</th>
			<th>D</th>
			<th>V</th>
		</tr>
	</thead>
	<tbody>
		@for (int i = 0; i < Model.csapatStatisztika.Count(); i++)
		{
			<tr>
				<td>
					@(i+1)
				</td>
				<td>@Model.csapatStatisztika[i].CsapatNev</td>
			</tr>
		}
	</tbody>
</table>
